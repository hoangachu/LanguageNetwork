<section class="formquestion">
    <div class="gap no-top">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <p class="textheader_v1"> Ask a public question</p>
                </div>
            </div>
        </div>
    </div>
</section>



<!-- contact info -->
<!-- footer -->

<div class="central-meta_v3 mt-2">
    <div class="new-postbox">
        @*<p class="">
                <b class="textheader"> Top Questions</b>

            </p>*@
        <div class="container">
            <div class="row">
                <ul>
                    <li>
                        <b>Title</b>
                    </li>
                    <li>
                        Be specific and imagine you’re asking a question to another person
                    </li>
                    <li>
                        <input id="title" name="title" type="text" maxlength="300" tabindex="100"
                               placeholder="e.g. Is there an R function for finding the index of an element in a vector?"
                               class="s-input js-post-title-field" value="" data-min-length="15" data-max-length="150" autocomplete="off" required>
                    </li>
                </ul>

            </div>
            <div class="row">
                <ul>
                    <li>
                        <b>Body</b>
                    </li>
                    <li>
                        Include all the information someone would need to answer your question
                    </li>
                    <li>
                        @{ Html.RenderPartial("PartialEditor"); }

                    </li>
                </ul>

            </div>
            <div class="row">
                <ul>
                    <li>
                        <b>Tags</b>
                    </li>
                    <li>
                        Add up to 5 tags to describe what your question is about
                    </li>
                    <li>
                        <input type="text" autocomplete="off" tabindex="103" placeholder="e.g. (spring vba python)"
                               id="tageditor-replacing-tagnames--input" class="s-input js-tageditor-replacing" style="width: 100%" required>
                    </li>
                </ul>

            </div>
        </div>
    </div>


</div>
<section class="formquestion">
    <div class="gap no-top">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <button id="btnQuestion">Post your question</button>
                </div>
            </div>
        </div>
    </div>
</section>




